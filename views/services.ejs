        /* Services Page Specific Styles */
body {
    background-color: #a8d1ff; /* Main blue background */
}

.main-content {
    padding-top: 100px;
    color: #643c94;
    width: 100%;
    max-width: 100%;
    margin: 0;
    overflow-x: hidden;
    position: relative;
}

.services-hero {
    background-color: #643c94;
    clip-path: polygon(0 0, 0 85%, 100% 95%, 100% 0);
    padding: 120px 0 200px;
    color: white;
    width: 100vw;
    max-width: 100%;
    margin-left: 0;
    margin-right: 0;
    position: relative;
    left: 0;
    right: 0;
    overflow-x: hidden;
}

.services-intro {
    max-width: 800px;
    margin: 0 auto;
    text-align: center;
    padding: 0 20px;
    position: relative;
    z-index: 2;
    box-sizing: border-box;
}

.services-intro h1 {
    font-size: 2.5rem;
    margin-bottom: 20px;
}

/* Content Section */
.main-content-area {
    max-width: 1200px;
    margin: -50px auto 50px;
    padding: 20px;
    position: relative;
    z-index: 2;
}

/* Area of Focus Section */
.area-focus-section {
    text-align: center;
    margin-bottom: 80px; /* Increased margin to create more space */
}

.area-focus-title {
    font-size: 1.8rem;
    color: #333;
    margin-bottom: 50px; /* Increased from 30px to 50px to add more space */
    font-weight: bold;
}

.focus-cards-container {
    display: flex;
    justify-content: center;
    gap: 40px; /* Increased from 20px to 40px to add more space between boxes */
    flex-wrap: wrap;
    margin-bottom: 40px;
}

.focus-card {
    background-color: #f3eb2e;
    border-radius: 15px;
    padding: 20px;
    width: 250px; /* Increased from 180px to 250px to make boxes wider */
    height: 80px;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-weight: bold;
    color: #000;
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.focus-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 15px rgba(0, 0, 0, 0.15);
}

/* Signature Programs Section */
.signature-programs-section {
    text-align: center;
    margin-top: 60px; /* Added more space above the section */
    margin-bottom: 60px; /* Added more space below the section */
}

.signature-programs-title {
    font-size: 1.8rem;
    color: #333;
    margin-bottom: 50px; /* Increased space between title and boxes */
    font-weight: bold;
}

.programs-container {
    display: flex;
    justify-content: center;
    gap: 30px; /* Increased gap between cards */
    flex-wrap: wrap;
    margin-top: 20px;
}

.program-card {
    border: 1px solid #ccc;
    border-radius: 10px;
    overflow: hidden;
    width: 320px; /* Increased width further */
    background-color: #e6f0f9;
    display: flex;
    flex-direction: column;
    margin: 0 15px; /* Added more margin between cards */
}

.program-image {
    height: 150px; /* Increased image area height */
    background-color: #ccc;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #666;
}

.program-content {
    padding: 20px; /* Increased padding */
    color: #333;
    text-align: left;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
}

.program-content h3 {
    margin-bottom: 30px; /* Increased from 20px to 30px to add more space between title and first bullet */
    color: #643c94;
    font-size: 1.4rem; /* Larger font size */
    font-weight: bold;
    text-align: center; /* Center the title text */
}

.program-content ul {
    list-style-type: disc; /* Add bullet points */
    padding-left: 20px; /* Indent the bullets */
    margin: 0 0 40px 0; /* Increased bottom margin to create space before button */
    flex-grow: 1;
}

.program-content li {
    margin-bottom: 12px; /* Space between items */
    position: relative;
    padding-left: 5px; /* Reduced padding since we now have real bullets */
    font-size: 1rem;
    line-height: 1.4;
}

/* Remove the before pseudo-element since we're using real bullets */
.program-content li:before {
    content: none;
}

.whatsapp-button {
    background-color: #643c94; /* Changed to purple */
    color: white;
    text-decoration: none;
    padding: 12px 15px; /* Larger padding */
    border-radius: 5px;
    text-align: center;
    font-weight: bold;
    margin-top: 20px; /* Add more space above the button */
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
}

.whatsapp-button:hover {
    background-color: #4e2e73; /* Darker purple on hover */
    transform: translateY(-2px);
}

.whatsapp-icon {
    margin-right: 8px;
    width: 16px;
    height: 16px;
}

.program-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

